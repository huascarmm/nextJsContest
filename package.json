{
  "name": "frontend.nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=none node server",
    "dev:staging": "cross-env NODE_ENV=development node server",
    "build:dev": "cross-env NODE_BRANCH=development next build",
    "build": "cross-env NODE_BRANCH=production next build",
    "start:test": "cross-env NODE_ENV=production NODE_BRANCH=development PORT=7070 node server test",
    "start:dev": "cross-env NODE_ENV=production NODE_BRANCH=development PORT=7070 node server",
    "start": "cross-env NODE_ENV=production NODE_BRANCH=production node server",
    "export:dev": "rm -rf ./out && cross-env NODE_EXPORT=true NODE_BRANCH=development next export",
    "export:prod": "rm -rf ./out && cross-env NODE_EXPORT=true NODE_BRANCH=production next export",
    "analyze": "cross-env BUNDLE_ANALYZE=true next build",
    "performance": "node lighthouse http://localhost:7070 && node performance"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
